<Page xmlns="http://schemas.nativescript.org/tns.xsd" xmlns:ui="@wuilmerj24/list-pager" navigatingTo="navigatingTo" class="page">
    <Page.actionBar>
        <ActionBar title="list-pager" icon="" class="action-bar">
        </ActionBar>
    </Page.actionBar>
    <StackLayout>
		<ui:ListPager items="{{items}}" width="100%" height="200">
			<ui:ListPager.itemTemplate>
                <GridLayout backgroundColor="blue">
                    <!-- <Image src="res://icon" stretch="aspectFit" width="100%" height="100%"/> -->
                    <Label text="{{title}}" textWrap="true" />
                    
                </GridLayout>
            </ui:ListPager.itemTemplate>
        </ui:ListPager>
    </StackLayout>
</Page>